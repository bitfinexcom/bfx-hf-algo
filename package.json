{
  "name": "bfx-hf-algo",
  "version": "4.0.3",
  "description": "HF Algorithmic Order Module",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)",
    "Anton Nazarenko <anton@code-care.pro>"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && npm run unit",
    "unit": "NODE_ENV=test mocha -R spec -b --recursive test/lib",
    "integration": "NODE_ENV=test mocha -R spec -b --recursive --timeout=60000 test/integration",
    "docs": "rm -rf docs && node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "benchmark": "clinic doctor --dest benchmarks/reports/$TARGET --on-port 'autocannon -d 30 localhost:$PORT' -- node --inspect ./benchmarks/$TARGET.js",
    "benchmark:visualize": "clinic doctor --visualize-only $REPORT",
    "postbenchmark:visualize": "open-cli $REPORT.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-hf-algo.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-hf-algo/issues"
  },
  "keywords": [
    "honey framework",
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "dependencies": {
    "@bitfinex/lib-js-util-math": "git+https://github.com/bitfinexcom/lib-js-util-math.git",
    "bfx-api-node-core": "^1.3.1",
    "bfx-api-node-models": "^1.1.9",
    "bfx-api-node-plugin-managed-candles": "^1.0.2",
    "bfx-api-node-plugin-managed-ob": "^1.0.2",
    "bfx-api-node-plugin-wd": "^1.0.4",
    "bfx-api-node-util": "^1.0.2",
    "bfx-hf-indicators": "^2.0.8",
    "bfx-hf-util": "^1.0.1",
    "bluebird": "^3.5.5",
    "debug": "^4.3.1",
    "flat-promise": "^1.0.2",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "autocannon": "^7.3.0",
    "bfx-hf-ext-plugin-bitfinex": "^1.0.5",
    "bfx-hf-models": "^2.1.0",
    "bfx-hf-models-adapter-lowdb": "^1.0.0",
    "chai": "^4.2.0",
    "clinic": "^9.0.0",
    "docdash": "^1.2.0",
    "dotenv": "^8.2.0",
    "jsdoc": "^3.6.3",
    "mocha": "^7.1.0",
    "open-cli": "^6.0.1",
    "restify": "^8.5.1",
    "sinon": "^9.0.0",
    "standard": "^16.0.3",
    "ws": "^7.4.5"
  },
  "standard": {
    "ignore": [
      "/docs/**/*.js"
    ]
  }
}
