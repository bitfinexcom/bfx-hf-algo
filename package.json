{
  "name": "bfx-hf-algo",
  "version": "2.1.1",
  "description": "HF Algorithmic Order Module",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)",
    "Anton Nazarenko <anton@code-care.pro>"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "lint": "./node_modules/.bin/eslint lib/ examples/ test/ index.js",
    "test": "npm run lint && npm run unit",
    "unit": "NODE_ENV=test NODE_PATH=lib/ SKIP_LIVE_TESTS=1 nyc --check-coverage --lines 60 --branches 40 --functions 40 --statements 60 --reporter=lcov --reporter=html mocha -b --recursive",
    "unit-live": "NODE_ENV=test NODE_PATH=lib/ nyc --check-coverage --lines 60 --branches 40 --functions 40 --statements 60 --reporter=lcov --reporter=html mocha -b --recursive",
    "docs": "rm docs/reference.md && ./node_modules/.bin/jsdoc2md -f index.js 'lib/**/*.js' > docs/reference.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-hf-algo.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-hf-algo/issues"
  },
  "keywords": [
    "honey framework",
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "dependencies": {
    "bfx-api-mock-srv": "^1.0.4",
    "bfx-api-node-core": "^1.1.8",
    "bfx-api-node-models": "^1.1.9",
    "bfx-api-node-plugin-managed-candles": "^1.0.0",
    "bfx-api-node-plugin-managed-ob": "^1.0.0",
    "bfx-api-node-plugin-wd": "^1.0.3",
    "bfx-api-node-util": "^1.0.2",
    "bfx-hf-indicators": "^2.0.0",
    "bfx-hf-models": "^2.1.0",
    "bfx-hf-util": "^1.0.1",
    "bitfinex-api-node": "^4.0.10",
    "bluebird": "^3.5.5",
    "debug": "^4.1.1",
    "eventemitter2": "^6.0.0",
    "lodash": "^4.17.15",
    "lodash.throttle": "^4.1.1",
    "lowdb": "^1.0.0",
    "p-iteration": "^1.1.7"
  },
  "devDependencies": {
    "bfx-hf-ext-plugin-bitfinex": "^1.0.5",
    "bfx-hf-models-adapter-lowdb": "^1.0.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "jsdoc-to-markdown": "^6.0.1",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^27.0.5",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "sinon": "^9.0.0",
    "socks-proxy-agent": "^5.0.0"
  }
}
