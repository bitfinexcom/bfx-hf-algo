{
  "name": "bfx-hf-algo",
  "version": "2.0.5",
  "description": "HF Algorithmic Order Module",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)",
    "Anton Nazarenko <anton@code-care.pro>"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && npm run unit",
    "unit": "DEBUG='bfx:hf:*' NODE_PATH=lib/ NODE_ENV=test istanbul cover _mocha -- -R spec -b --recursive",
    "test-without-coverage": "NODE_ENV=test mocha -R spec -b --recursive",
    "macrossover_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/ma_crossover/index.js > docs/ma_crossover.md",
    "aohost_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/ao_host.js > docs/ao_host.md",
    "helpers_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/host/gen_helpers.js > docs/helpers.md",
    "iceberg_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/iceberg/index.js > docs/iceberg.md",
    "twap_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/twap/index.js > docs/twap.md",
    "pingpong_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/ping_pong/index.js > docs/ping_pong.md",
    "ad_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/accumulate_distribute/index.js > docs/accumulate_distribute.md",
    "docs": "npm run aohost_docs && npm run helpers_docs && npm run iceberg_docs && npm run twap_docs && npm run ad_docs && npm run macrossover_docs && npm run pingpong_docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-hf-algo.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-hf-algo/issues"
  },
  "keywords": [
    "honey framework",
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "dependencies": {
    "bfx-api-mock-srv": "^1.0.0",
    "bfx-api-node-core": "^1.1.0",
    "bfx-api-node-models": "^1.0.12",
    "bfx-api-node-plugin-managed-candles": "^1.0.0",
    "bfx-api-node-plugin-managed-ob": "^1.0.0",
    "bfx-api-node-plugin-wd": "^1.0.0",
    "bfx-api-node-util": "^1.0.2",
    "bfx-hf-indicators": "^2.0.0",
    "bfx-hf-models": "^2.1.0",
    "bfx-hf-util": "^1.0.1",
    "bitfinex-api-node": "^2.0.9",
    "bluebird": "^3.5.5",
    "debug": "^4.1.1",
    "eventemitter2": "^5.0.1",
    "lodash": "^4.17.15",
    "lodash.throttle": "^4.1.1",
    "lowdb": "^1.0.0",
    "p-iteration": "^1.1.7"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "bfx-hf-ext-plugin-bitfinex": "^1.0.0",
    "bfx-hf-models-adapter-lowdb": "^1.0.0",
    "chai": "^4.2.0",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^7.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^6.2.0",
    "socks-proxy-agent": "^4.0.1",
